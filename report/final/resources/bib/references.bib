@article{dongEstimationDaytimeNet1992,
  ids = {dongEstimationDaytimeNet1992a},
  title = {Estimation of {{Daytime Net Radiation Over Well-Watered Grass}}},
  author = {Dong, A. and Grattan, Steve and Carroll, J. and Prashar, C.},
  year = {1992},
