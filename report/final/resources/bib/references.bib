  title = {Global Terrestrial Water Storage and Drought Severity under Climate Change},
  author = {Pokhrel, Yadu and Felfelani, Farshid and Satoh, Yusuke and Boulange, Julien and Burek, Peter and G{\"a}deke, Anne and Gerten, Dieter and Gosling, Simon N. and Grillakis, Manolis and Gudmundsson, Lukas and Hanasaki, Naota and Kim, Hyungjun and Koutroulis, Aristeidis and Liu, Junguo and Papadimitriou, Lamprini and Schewe, Jacob and M{\"u}ller Schmied, Hannes and Stacke, Tobias and Telteu, Camelia-Eliza and Thiery, Wim and Veldkamp, Ted and Zhao, Fang and Wada, Yoshihide},
  year = {2021},
