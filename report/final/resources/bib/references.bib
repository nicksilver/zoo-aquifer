  author = {Lange, Stefan and Volkholz, Jan and Geiger, Tobias and Zhao, Fang and Vega, Iliusi and Veldkamp, Ted and Reyer, Christopher P. O. and Warszawski, Lila and Huber, Veronika and J{\"a}germeyr, Jonas and Schewe, Jacob and Bresch, David N. and B{\"u}chner, Matthias and Chang, Jinfeng and Ciais, Philippe and Dury, Marie and Emanuel, Kerry and Folberth, Christian and Gerten, Dieter and Gosling, Simon N. and Grillakis, Manolis and Hanasaki, Naota and Henrot, Alexandra-Jane and Hickler, Thomas and Honda, Yasushi and Ito, Akihiko and Khabarov, Nikolay and Koutroulis, Aristeidis and Liu, Wenfeng and M{\"u}ller, Christoph and Nishina, Kazuya and Ostberg, Sebastian and M{\"u}ller Schmied, Hannes and Seneviratne, Sonia I. and Stacke, Tobias and Steinkamp, J{\"o}rg and Thiery, Wim and Wada, Yoshihide and Willner, Sven and Yang, Hong and Yoshikawa, Minoru and Yue, Chao and Frieler, Katja},
  year = {2020},
