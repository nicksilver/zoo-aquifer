@article{forero-ortizClimateChangeImplications2020,
  title = {Climate {{Change Implications}} for {{Water Availability}}: {{A Case Study}} of {{Barcelona City}}},
  shorttitle = {Climate {{Change Implications}} for {{Water Availability}}},
  author = {{Forero-Ortiz}, Edwar and {Mart{\'i}nez-Gomariz}, Eduardo and Monjo, Robert},
